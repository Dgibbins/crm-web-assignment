<h1>All Contacts</h1>
<ul>
  <% Contact.all.each do |contact| %>

    <li><a href="/contacts/<%=contact.id%>"><%= contact.first_name %>  <%= contact.last_name %>  <%= contact.email%>  <%= contact.number%>  <%= contact.note%></a></li>
     <li> <a href="contacts/<%=contact.id%>/edit">Edit</a> | <a href="#">Delete</a> </li>

    <% end %>

</ul>
