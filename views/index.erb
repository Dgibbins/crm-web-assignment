    <p>There are <a href="/contacts"><%= Contact.all.length%></a> contacts </p>

    <ul>
        <% Contact.all.each do |contact| %>
        <div class="contact-list">
          <li><%= contact.first_name %>  <%= contact.last_name %>  <%= contact.email%>  <%= contact.number%></li>
          <li><a href="#">Edit</a> | <a href="#">Delete</a></li>
        </div>
        <% end %>
    </ul>
